{
  "name": "Add Category",
  "model": "category",
  "fields": [
    {
      "name": "Category Post",
      "label": "Category Post",
      "model": "title",
      "type": "text",
      "require": true,
      "placeholder": "Category Post"
    },
    {
      "name": "Name Category",
      "label": "Name Category",
      "model": "name",
      "placeholder": "Category Name",
      "type": "text",
      "require": true
    },
    {
      "name": "H1 Category",
      "label": "H1 Category",
      "model": "h1",
      "type": "text",
      "require": true,
      "placeholder": "H1"
    },
    {
      "name": "Status Post",
      "label": "Status Post",
      "model": "status",
      "type": {
        "select": {
          "options": "status"
        }
      },
      "require": true,
      "placeholder": "Category Status"
    },
    {
      "name": "Type Component",
      "label": "Template",
      "model": "componentName",
      "type": {
        "select": {
          "options": [
            "category",
            "research-list",
            "news"
          ]
        }
      },
      "require": true,
      "placeholder": "Category Status"
    },
    {
      "name": "Desc Category",
      "label": "Desc Category",
      "model": "description",
      "type": "textarea",
      "require": true,
      "placeholder": "Explain what the Category is talking about"
    },
    {
      "name": "Cover Category",
      "label": "Cover Category",
      "model": "image_large",
      "type": "image",
      "require": true
    },
    {
      "name": "Small Category",
      "label": "Small Category",
      "model": "image_small",
      "type": "image",
      "require": true
    },
    {
      "name": "Category Content",
      "label": "Category Content",
      "model": "content",
      "type": "content",
      "require": true
    }
  ]
}