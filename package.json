  "scripts": {
    "dev": "cross-env NODE_ENV=development NUXT_TELEMETRY_DISABLED=1 nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production NUXT_TELEMETRY_DISABLED=1 node server/index.js",
    "heroku-postbuild": "yarn run build",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "dev-spa": "nuxt dev --spa",
    "build-spa": "nuxt build --spa",
    "start-spa": "nuxt start --spa",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.0",
    "nuxt": "^2.14.12",
    "cross-env": "^5.2.0"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/pwa": "^3.3.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/vuetify": "^1.11.2",
    "babel-eslint": "^10.0.3",
    "babel-plugin-component": "^1.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.0.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "nodemon": "^1.18.9"
  }
}
